<p id="notice"><%= notice %></p>

<h1>Listing Datasets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datasets.each do |dataset| %>
      <tr>
        <td><%= dataset.title %></td>
        <td><%= dataset.user_id %></td>
        <td><%= link_to 'Show', dataset_path(dataset.id) %></td>
        <td><%= link_to 'Edit', edit_dataset_path(dataset.id) %></td>
        <td><%= link_to 'Destroy', dataset_path(dataset.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dataset', new_dataset_path %>
